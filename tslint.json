{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": true,
    "typedef": [true, "variable-declaration", "call-signature"],
    "semicolon": [true, "always"]
  }
}
